<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="gumstix" revision="default"/>

  <!-- You can have as many remote repositories as you like -->

  <remote name="yocto" fetch="https://git.yoctoproject.org" />
  <remote name="oe" fetch="https://git.openembedded.org" />
  <remote name="gumstix" fetch="https://github.com/gumstix" />
  <remote name="qt5" fetch="https://github.com/meta-qt5" />
  <remote name="qt5-extra" fetch="https://github.com/schnitzeltony" />
  <remote name="tegra" fetch="https://github.com/madisongh" />
  <remote name="ossytems" fetch="https://github.com/OSSystems" />
  <remote name="clang" fetch="https://github.com/kraj" />
  <remote name="toradex" fetch="https://git.toradex.com" />
  <remote name="freescale" fetch="https://github.com/Freescale" />
  <remote name="bmwcarit" fetch="https://github.com/bmwcarit" />

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/kirkstone" remote="yocto" />
  <project name="meta-ti" revision="refs/heads/kirkstone"  remote="yocto" path="poky/meta-ti" />
  <project name="meta-openembedded" revision="refs/heads/kirkstone"  remote="oe" path="poky/meta-openembedded" />
  <project name="meta-gumstix" revision="refs/heads/dunfell"  remote="gumstix" path="poky/meta-gumstix" />
  <project name="meta-gumstix-extras" revision="refs/heads/dunfell" remote="gumstix" path="poky/meta-gumstix-extras" />
 
</manifest>

