<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="gumstix" revision="default"/>

  <!-- You can have as many remote repositories as you like -->

  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="gumstix" fetch="git://github.com/gumstix" />
  <remote name="qt5" fetch="git://github.com/meta-qt5" />
  <remote name="tegra" fetch="git://github.com/madisongh" />

  <!-- List all the repositories you want to fetch -->
  <!-- "name": git repository's name on hosting server
       "remote": the remote host from above (if not the default)
       "revision": commitish to checkout (if not the default)
       "path": directory where git repository gets cloned/synced -->

  <project name="poky" revision="refs/heads/warrior" remote="yocto" />
  <project name="meta-ti" revision="refs/heads/master"  remote="yocto" path="poky/meta-ti" />
  <project name="meta-raspberrypi" revision="refs/heads/warrior"  remote="yocto" path="poky/meta-raspberrypi" />
  <project name="meta-openembedded" revision="refs/heads/warrior"  remote="oe" path="poky/meta-openembedded" />
  <project name="meta-gumstix" revision="refs/heads/warrior"  remote="gumstix" path="poky/meta-gumstix" />
  <project name="meta-gumstix-extras" revision="refs/heads/warrior" remote="gumstix" path="poky/meta-gumstix-extras" />
  <project name="meta-qt5" revision="refs/heads/warrior"  remote="qt5" path="poky/meta-qt5" />
  <project name="meta-tegra" revision="refs/heads/warrior-l4t-r32.2" remote="tegra" path="poky/meta-tegra" />
  <project name="meta-java" revision="refs/heads/warrior"  remote="yocto" path="poky/meta-java" />
  <project name="meta-tensorflow" revision="refs/heads/master"  remote="yocto" path="poky/meta-tensorflow" />

</manifest>

